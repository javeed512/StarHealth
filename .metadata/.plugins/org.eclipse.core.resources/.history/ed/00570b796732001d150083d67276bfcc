package com.starhealth.variables;

public class Employee {
	
	int eid;
	String ename;
	

	static String  job;
	
	public static void main(String[] args) {
		
		Employee  e1 =		new  Employee();

			System.out.println(e1.eid);
			System.out.println(e1.ename);
			
			System.out.println(job);

	}

	
	public  void  display() {
		
		
		System.out.println(job);
	}
	
	
}
